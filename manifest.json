{
  "manifest_version": 3,
  "name": "YouTube Ad Speed Booster",
  "version": "1.0",
  "description": "Automatically speeds up YouTube ads.",
  "permissions": ["scripting"],
  "host_permissions": ["*://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
